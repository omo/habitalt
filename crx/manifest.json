{
  "name": "Habitalt",
  "version": "0.1.2",
  "manifest_version": 2,
  "description": "An anti-habitual site blocker.",
  "icons": {
    "16" : "icon16.png",
    "48" : "icon48.png",
    "128": "icon128.png"
  },

 "options_page": "options.html",
 "background": {
    "page": "background.html"
  },
  "permissions": [
    "webRequest", "webRequestBlocking", "tabs", "background", "http://*/*", "https://*/*"
  ]
}